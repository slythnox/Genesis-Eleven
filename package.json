{
  "name": "genesis-eleven-cli",
  "version": "1.0.0",
  "description": "AI-powered command-line assistant for natural-language desktop control",
  "type": "module",
  "bin": {
    "el": "./bin/el.js"
  },
  "scripts": {
    "start": "node ./bin/el.js",
    "test": "jest --testMatch='**/*.test.js'",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "dev": "node --watch ./bin/el.js",
    "build": "echo 'No build step required for Node.js CLI'",
    "clean": "rm -rf logs/ node_modules/.cache"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.17.1",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "fs-extra": "^11.2.0",
    "chalk": "^5.3.0",
    "prompts": "^2.4.2",
    "ora": "^7.0.1",
    "retry": "^0.13.1",
    "yaml": "^2.3.4",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "@jest/globals": "^29.7.0"
  },
  "keywords": [
    "cli",
    "ai",
    "assistant",
    "desktop-control",
    "automation", 
    "gemini",
    "command-line",
    "natural-language",
    "system-control"
  ],
  "author": "Slythnox",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/slythnox/genesis-eleven-cli.git"
  },
  "bugs": {
    "url": "https://github.com/slythnox/genesis-eleven-cli/issues"
  },
  "homepage": "https://github.com/slythnox/genesis-eleven-cli#readme"
}